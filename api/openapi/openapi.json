{
  "openapi": "3.1.0",
  "info": { "title": "FastAPI", "version": "0.1.0" },
  "paths": {
    "/video": {
      "post": {
        "tags": ["video"],
        "summary": "Create Video",
        "operationId": "create_video_video_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/VideoIn" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/VideoOut" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/video/{perma_token}": {
      "get": {
        "tags": ["video"],
        "summary": "Read Video",
        "operationId": "read_video_video__perma_token__get",
        "parameters": [
          {
            "name": "perma_token",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid",
              "title": "Perma Token"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/VideoOut" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      },
      "put": {
        "tags": ["video"],
        "summary": "Replace Video",
        "operationId": "replace_video_video__perma_token__put",
        "parameters": [
          {
            "name": "perma_token",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid",
              "title": "Perma Token"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/VideoIn" }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "integer",
                  "title": "Response Replace Video Video  Perma Token  Put"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Team": {
        "properties": {
          "name": { "type": "string", "maxLength": 30, "title": "Team's name" },
          "color": {
            "anyOf": [
              { "type": "string", "maxLength": 15 },
              { "type": "null" }
            ],
            "title": "Jersey's main color",
            "examples": ["#294cbe"]
          }
        },
        "type": "object",
        "maxLength": 30,
        "required": ["name"],
        "title": "Team"
      },
      "Teams": {
        "properties": {
          "dark": {
            "properties": {
              "name": {
                "type": "string",
                "maxLength": 30,
                "title": "Team's name"
              },
              "color": {
                "anyOf": [
                  { "type": "string", "maxLength": 15 },
                  { "type": "null" }
                ],
                "title": "Jersey's main color",
                "examples": ["#294cbe"]
              }
            },
            "type": "object",
            "maxLength": 30,
            "required": ["name"],
            "title": "Dark team"
          },
          "light": {
            "properties": {
              "name": {
                "type": "string",
                "maxLength": 30,
                "title": "Team's name"
              },
              "color": {
                "anyOf": [
                  { "type": "string", "maxLength": 15 },
                  { "type": "null" }
                ],
                "title": "Jersey's main color",
                "examples": ["#294cbe"]
              }
            },
            "type": "object",
            "maxLength": 30,
            "required": ["name"],
            "title": "Light team"
          }
        },
        "type": "object",
        "required": ["dark", "light"],
        "title": "Teams"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      },
      "VideoIn": {
        "properties": {
          "src": {
            "type": "string",
            "maxLength": 3000,
            "title": "Source video's url"
          },
          "teams": { "$ref": "#/components/schemas/Teams" },
          "event": {
            "anyOf": [
              { "type": "string", "maxLength": 50 },
              { "type": "null" }
            ],
            "title": "Context event of the game",
            "examples": ["French championship 2024"]
          }
        },
        "type": "object",
        "required": ["src", "teams"],
        "title": "VideoIn"
      },
      "VideoOut": {
        "properties": {
          "src": {
            "type": "string",
            "maxLength": 3000,
            "title": "Source video's url"
          },
          "teams": { "$ref": "#/components/schemas/Teams" },
          "event": {
            "anyOf": [
              { "type": "string", "maxLength": 50 },
              { "type": "null" }
            ],
            "title": "Context event of the game",
            "examples": ["French championship 2024"]
          },
          "perma_token": {
            "type": "string",
            "format": "uuid",
            "title": "Permanent ID of this augmented video object."
          }
        },
        "type": "object",
        "required": ["src", "teams"],
        "title": "VideoOut"
      }
    }
  }
}
